					<xs:any maxOccurs="unbounded" minOccurs="0" namespace="##other" processContents="lax"/>
				</xs:sequence>
			</xs:complexType>

			<xs:complexType name="RefundInfoList">
				
				<xs:sequence>
					<xs:element maxOccurs="unbounded" minOccurs="1" name="refundInfo" type="ap:RefundInfo">
						
					</xs:element>
				</xs:sequence>
