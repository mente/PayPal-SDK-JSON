				<xs:annotation>
					<xs:documentation>
						Describes a payment for a particular receiver
						(merchant), contains list of additional per item
						details.
					</xs:documentation>
					
				</xs:annotation>
				<xs:sequence>
					<xs:element name="item" type="ap:InvoiceItem" minOccurs="0" maxOccurs="unbounded">
						
					</xs:element>
					<xs:element name="totalTax" type="xs:decimal" minOccurs="0" maxOccurs="1">
						
					</xs:element>
					<xs:element name="totalShipping" type="xs:decimal" minOccurs="0" maxOccurs="1">
						
					</xs:element>
					<xs:any maxOccurs="unbounded" minOccurs="0" namespace="##other" processContents="lax"/>
				</xs:sequence>
			</xs:complexType>


			<xs:complexType name="PayError">
				<xs:annotation>
					<xs:documentation>
