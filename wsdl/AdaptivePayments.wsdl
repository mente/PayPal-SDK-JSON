			<xs:complexType name="Address">
				
				<xs:sequence>
					<xs:element name="addresseeName" type="xs:string" maxOccurs="1" minOccurs="0">
						
					</xs:element>
					<xs:element name="baseAddress" type="common:BaseAddress" maxOccurs="1" minOccurs="1">
						
					</xs:element>
					<xs:element name="addressId" type="xs:string" minOccurs="0" maxOcurrs="1">
						
					</xs:element>
					<xs:any maxOccurs="unbounded" minOccurs="0" processContents="lax" namespace="##other"/>
				</xs:sequence>
			</xs:complexType>

			<xs:complexType name="AddressList">
				
				<xs:sequence>
					<xs:element name="address" type="ap:Address" maxOccurs="unbounded" minOccurs="1">
						
					</xs:element>
				</xs:sequence>
			</xs:complexType>

			<xs:complexType name="CurrencyCodeList">
				<xs:annotation>
					<xs:documentation>
						A list of ISO currency codes.
					</xs:documentation>
					
				</xs:annotation>
				<xs:sequence>
					<xs:element minOccurs="1" maxOccurs="unbounded" name="currencyCode" type="xs:string">
						
					</xs:element>
				</xs:sequence>
			</xs:complexType>

			<xs:complexType name="CurrencyConversionList">
				<xs:annotation>
					<xs:documentation>
						A list of estimated currency conversions for a
						base currency.
					</xs:documentation>
					
				</xs:annotation>
				<xs:sequence>
					<xs:element minOccurs="1" maxOccurs="1" name="baseAmount" type="common:CurrencyType">
						
					</xs:element>
					<xs:element minOccurs="1" maxOccurs="1" name="currencyList" type="ap:CurrencyList">
						
					</xs:element>
				</xs:sequence>
			</xs:complexType>

			<xs:complexType name="CurrencyConversionTable">
				<xs:annotation>
					<xs:documentation>
						A table that contains a list of estimated
						currency conversions for a base currency in each
						row.
					</xs:documentation>
					
				</xs:annotation>
				<xs:sequence>
					<xs:element minOccurs="1" maxOccurs="unbounded" name="currencyConversionList" type="ap:CurrencyConversionList">
						
					</xs:element>
				</xs:sequence>
			</xs:complexType>

			<xs:complexType name="CurrencyList">
				<xs:annotation>
					<xs:documentation>
						A list of ISO currencies.
